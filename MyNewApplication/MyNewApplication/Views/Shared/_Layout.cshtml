<!DOCTYPE html>
<html>
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - KCS IT GLOBAL</title>

    @Scripts.Render("~/bundles/js/jquery")
    @Styles.Render("~/Content/css/bootstrap")
    @Scripts.Render("~/bundles/js/CreatNewAccount")
    @Scripts.Render("~/bundles/Js/UserRecord")
    @Styles.Render("~/Content/css/site")
    @Scripts.Render("~/bundles/js/modernizr")

    @*Bootstrap CSS CDN Link*@
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" type="text/css" />

    @*Font Awesome CSS CDN Link*@
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />

    @*Bootstrap JS CDN Link*@
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js" type="text/javascript"></script>

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Demo MVC ", "CreateNewAccount", "Index", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Create New Account ", "CreateNewAccount", "Index")</li>
                    <li>@Html.ActionLink("User Record", "UserRecord", "Index")</li>
                    <li>@Html.ActionLink("Chart", "Chart", "Index")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year -DEVELOPED BY KCS IT GLOBAL  </p>
        </footer>
    </div>

    
    @RenderSection("scripts", required: false)
</body>
</html>
